<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Random Survey Redirect</title>
  <script>
    // Define the list of survey URLs
    const surveys = [
      "https://docs.google.com/forms/d/1iXZOzlSk1Oz8p6fykppulhWLXZcz_z3PuyJXDaf7gig/edit", // Study 2 - Pretest 2.1 (Adv x Linear)
      "https://docs.google.com/forms/d/1CpSBlaiZzsoUdwItvHv0yj9FNdMdkxFY6Trnzn8jGqo/edit" // Study 2 - Pretest 2.2 (Adv x Non-linear)
      "https://docs.google.com/forms/d/1JvZexIp4ZwHCzZya7rUSIypl1Aopn024_7c2Es7JOyw/edit", // Study 2 - Pretest 2.3 (Transf x Linear)
      "https://docs.google.com/forms/d/1cHwbxNXL3jgyyQbpwV0J1X-_96IbwGO4sgn4LUJt4Sw/edit" // Study 2 - Pretest 2.4 (Transf x Non-linear)
    ];

    // Shuffle and redirect
    window.onload = function () {
      const randomIndex = Math.floor(Math.random() * surveys.length);
      window.location.href = surveys[randomIndex];
    };
  </script>
</head>
<body>
  <p>Redirecting you to the survey...</p>
</body>
</html>
