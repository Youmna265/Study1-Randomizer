<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Random Survey Redirect</title>
  <script>
    // Define the list of survey URLs
    const surveys = [
      "https://docs.google.com/forms/d/1pCZYbL5RT5pwqGnJwuXXlvwHKBAgjs5nY0HkQoy6VXA/edit", // Study 2 - Pretest 2.1 (Adv x Linear)
      "https://docs.google.com/forms/d/1ARe7XNyjmQCZo1QC-LleCpWrXJUA7fZCXY4lL5ot7ms/edit" // Study 2 - Pretest 2.2 (Adv x Non-linear)
      "https://docs.google.com/forms/d/1JvZexIp4ZwHCzZya7rUSIypl1Aopn024_7c2Es7JOyw/edit", // Study 2 - Pretest 2.3 (Transf x Linear)
      "https://docs.google.com/forms/d/1XSOb8JSKAbkLERPYKkkcuk9HIw-shsYb-LD80IEqNZ0/edit" // Study 2 - Pretest 2.4 (Transf x Non-linear)
    ];

    // Shuffle and redirect
    window.onload = function () {
      const randomIndex = Math.floor(Math.random() * surveys.length);
      window.location.href = surveys[randomIndex];
    };
  </script>
</head>
<body>
  <p>Redirecting you to the survey...</p>
</body>
</html>
